params:
  rootOperation:
    value: b1s1
  sizeVarName:
    value: "inputSize"
  selectedPlan:
    value: "Usage limitations"
  param1:
    value: 1
  param2:
    value: 100
services:
  b1: ./b1-oas.yaml
  s1: ./s1-oas.yaml
  s2: ./s2-oas.yaml
mappings:
  - input:
      service: b1
      path: /api/b1/m1
      method: GET
    output:
      - service: s1
        path: /api/s1/m1
        method: GET
        count: param1
      - service: s1
        path: /api/s1/m2
        method: GET
        count: param1
  - input:
      service: s1
      path: /api/s1/m1
      method: GET
    output:
      - service: s2
        path: /api/s2/m1
        method: GET
        count: param1
  - input:
      service: s1
      path: /api/s1/m2
      method: GET
    output:
      - service: s2
        path: /api/s2/m1
        method: GET
        count: param1
##added
  - input:
      service: s2
      path: /api/s2/m1
      method: GET
    output:
      - service: s2
        path: /api/s2/m2
        method: GET
        count: 1
